  "description": "TaskNote App - Your daily-basis friend to help you with TODOs and Notes",
  "keywords": [
    "react",
    "typescript",
    "node",
    "nestjs"
  ],
  "repository": "https://github.com/ricardo-campos-org/react-typescript-todolist",
  "private": true,
  "dependencies": {
